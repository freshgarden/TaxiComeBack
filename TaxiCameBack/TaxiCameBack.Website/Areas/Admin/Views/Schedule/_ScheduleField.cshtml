@using TaxiCameBack.Website.Application
@model TaxiCameBack.Website.Areas.Admin.Models.ScheduleViewModel

<div class="form-group">
    @Html.LabelFor(model => model.BeginLocation)
    @Html.EditorFor(model => model.BeginLocation, new { htmlAttributes = new { @class = "form-control", placeholder = "Nhập điểm đi" } })
    @Html.ValidationMessageFor(model => model.BeginLocation)
</div>
<script src="http://www.jqueryscript.net/demo/Easy-Customizable-jQuery-Dropdown-Date-Picker-Plugin/dist/jquery.date-dropdowns.min.js"></script>
<div class="form-group">
    @Html.LabelFor(model => model.EndLocation)
    @Html.EditorFor(model => model.EndLocation, new { htmlAttributes = new { @class = "form-control", placeholder = "Nhập điểm đến" } })
    @Html.ValidationMessageFor(model => model.EndLocation)
</div>

<div class="form-group">
    @Html.LabelFor(model => model.StartDate)
    @Html.EditorFor(model => model.StartDate, new { htmlAttributes = new { @class = "form-control" } })
    @Html.ValidationMessageFor(model => model.StartDate)
</div>


<div>
    @Html.HiddenFor(model => model.ScheduleGeolocations)
</div>
